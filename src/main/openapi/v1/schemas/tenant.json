        "description": "A tenant",
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "[Optional] 8-character lower-case following base32",
            "minLength": 8,
            "maxLength": 8
          },
          "shortName": {
            "type": "string",
